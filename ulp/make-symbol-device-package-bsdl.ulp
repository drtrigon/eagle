              dlgCell(5, 1) dlgPushButton("PIN") rename_pin(n_pins, search, replace);  // 2011-04-01

              dlgCell(6, 0) dlgLabel("Rename ");
              dlgCell(6, 1) dlgPushButton("PAD")  rename_pad(n_pins, search, replace); // 2008-11-07

              dlgCell(7, 0) dlgLabel("Pads for pins");
              dlgCell(7, 1) dlgPushButton("numbering")  number_pads(n_pins);                  // 2008-07-03
            }
          }
          dlgStretch(1);
          dlgHBoxLayout {
            dlgPushButton("&Help") infohelp(HelpList); // 2011-04-01
            dlgStretch(1);
            if (strstr(OS_SIGNATURE, "Windows") == 0) {
              dlgPushButton("Manual") {
                sprintf(PdfDocLink, "%s/doc/make-symbol-device-package-bsdl-2011-en.pdf", EAGLE_DIR );
                if (language() == "de") {
                  sprintf(PdfDocLink, "%s/doc/make-symbol-device-package-bsdl-2011-de.pdf", EAGLE_DIR);
                }
                system_call(PdfDocLink);
              }
            }
            dlgLabel(Revision);
          }
        }
      }
    }
  };

  if (result) {
    if (make_Symbol)  cmd += "SCRIPT '" + symbol_file + "';\nWIN FIT;\n";
    if (make_Device)  cmd += "SCRIPT '" + device_file + "';\nWIN FIT;";
  }
  exit(cmd);
}

else {
  dlgMessageBox("!This is no Library!\nThis program can only work in the Library editor.");
  exit(0);
}